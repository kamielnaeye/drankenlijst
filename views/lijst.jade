html
head

body
    p(style= 'display:none',id='naamTabel') #{naam}
    p(style= 'display:none',id='prijsTabel') #{prijs}
    p(style= 'display:none',id='urlTabel') #{url}
table(id="tabel" class="table table table-striped table-hover")
    thead(class="thead-dark")
        tr
            th Lijst
            tr 
                td naam
                td Prijs
                td Foto 
    tbody
        tr(id="row")
script.
    var prijsTabel = document.getElementById("prijsTabel");
    var naamTabel = document.getElementById("naamTabel");
    var urlTabel = document.getElementById("urlTabel");    
    var prijs = prijsTabel.innerText.split(',');
    var naam = naamTabel.innerText.split(',');
    var url = urlTabel.innerText.split(',');
    var tabel = document.getElementById("tabel");
    for(var i= prijs.length-1;i>=0;i--){
        var row = tabel.insertRow();
        row.id="row";
        var cell1 = row.insertCell(0);
        var cell2 = row.insertCell(1);
        var cell3 = row.insertCell(2);        
        cell1.innerHTML = naam[i];
        cell2.innerHTML = prijs[i];
        cell3.innerHTML = '<img height="100" width="100" src="'+url[i]+'"></img>';
    }
